{
	"name": "thinktool",
	"version": "0.0.1",
	"main": "index.js",
	"repository": "https://github.com/PiersonBro/ThinkTool",
	"author": "Alyssa Fusato, Tyler Gamlem, Ezekiel Pierson",
	"private": false,
	"scripts": {
		"flow": "flow",
		"dev": "webpack serve",
		"prebuild": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"build": "rm -rf dist && webpack --mode production --config webpack.prod.config.js && webpack --mode production --config webpack.server.config.js",
		"test": "yarn prettier-check; yarn build",
		"start": "node ./dist/server.*.wp.js",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.js*\" \"**/*.*css\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different \"{*.md,./{src}/**/*.js?(x)}\"",
		"lint": "yarn prettier && yarn stylelint",
		"eslint": "eslint '*/**/*.{js,jsx}'",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"**/*.*css\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"**/*.*css\" --color --cache"
	},
	"browsersList": [
		"last 2 versions",
		"not dead",
		"> 0.2%"
	],
	"dependencies": {
		"express": "^4.17.1",
		"express-validator": "^6.7.0",
		"firebase": "^8.1.1",
		"firebaseui": "^4.7.1",
		"fs": "^0.0.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.8",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-flow": "^7.12.1",
		"@babel/preset-react": "^7.12.7",
		"@webpack-cli/serve": "^1.1.0",
		"babel-loader": "^8.2.1",
		"compression-webpack-plugin": "^6.1.1",
		"css-loader": "^5.0.1",
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.3",
		"file-loader": "^6.0.0",
		"flow-bin": "^0.138.0",
		"html-webpack-plugin": "^4.4.1",
		"mini-css-extract-plugin": "^1.3.1",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"path": "^0.12.7",
		"prettier": "^2.2.0",
		"react-is": "^17.0.1",
		"sass": "^1.29.0",
		"sass-loader": "^10.1.0",
		"style-loader": "^2.0.0",
		"stylelint": "^13.8.0",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-prettier": "^1.1.2",
		"terser-webpack-plugin": "^4.2.3",
		"webpack": "^4.44.2",
		"webpack-cli": "^4.2.0",
		"webpack-dev-server": "^3.11.0",
		"webpack-node-externals": "^2.5.2"
	}
}
